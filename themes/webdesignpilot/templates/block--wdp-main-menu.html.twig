{#
/**
 * @file
 * Bartik's theme implementation for a menu block.
 *
 * @ingroup themeable
 */
#}
{% block content %}
{# Descktop menu #}
<div class="navbar-desk">
	<nav class="nav-anim">
		{{ content }}	
	</nav>
</div>

{# mobile menu #}
<div class="navbar-mob">
  <div class="wpopen" onclick="openNav()"><i class="fa fa-bars" aria-hidden="true"></i> menu</div>
	<div id="myNav" class="overlay">
	  <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
	    <div class="overlay-content">
			<div class="wpnavbar" {{ content_attributes.addClass('content') }}>
			      {{ content }}
			</div>
		</div>
	</div>
</div>
{% endblock %}
